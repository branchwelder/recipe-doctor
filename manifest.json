{
  "manifest_version": 2,
  "name": "RecipeMD",
  "version": "1.0",
  "description": "Recipes to markdown.",
  "icons": {
    "48": "icons/border-48.png"
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "icons/border-48.png",
    "default_title": "RecipeMD",
    "default_popup": "popup/popup.html"
  }
}